{
  "name": "ohmyshares/broker-report-parser",
  "description": "Broker report parsing utility",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "phpoffice/phpspreadsheet": "^1.29"
  },
  "autoload": {
    "psr-4": {
      "ohmyshares\\brokerReportParser\\": "src/",
      "ohmyshares\\brokerReportParser\\tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  }
}
